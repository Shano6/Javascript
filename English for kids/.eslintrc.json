{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "import/extensions":"off",
        "prefer-destructuring":"off",
        "class-methods-use-this":"off",
        "no-param-reassign":"off"
    }
}
